    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "clean-css-cli": "^5.6.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
